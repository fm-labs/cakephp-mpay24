{
    "name": "fm-labs/cakephp-mpay24",
    "description": "Mpay24 plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "ext-soap": "*",
        "cakephp/cakephp": "^4.4",
        "mpay24/mpay24-php": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "FmLabs\\Mpay24\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FmLabs\\Mpay24\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
